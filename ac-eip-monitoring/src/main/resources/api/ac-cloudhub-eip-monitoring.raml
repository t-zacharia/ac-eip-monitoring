#%RAML 1.0
title: eip-cloudhub-monitoring
description: 'api for mule application health statistics per worker (cpu, memory etc.)'
version: 1.4
mediaType: application/json
protocols:
  - HTTP
baseUri: /eip/monitoring/
/dashboard/:
  get:
    queryParameters: 
      eipEnvId:
        displayName: Environment ID
        type: string
        description: Idenfier for the environment
        example: 374b1397-0fa0-4611-bea7-1c84ed78fa60
        required: false
      startDate:
        displayName: Start date
        type: string
        description: ISO format date string representing the last date to retrieve statistics
        example: 2018-06-09T04:57:28.572Z
        required: true      
      endDate:
        displayName: End date
        type: string
        description: ISO format date string representing the start date to retrieve statistics
        example: 2018-06-09T05:57:28.572Z
        required: true
      interval:
        displayName: Interval
        type: string
        description: Amount of time between samples in milliseconds
        example: "14400000"
        required: true
    responses:
      '200':
        description: Returns the list of applications and the usage statistics for each application and its associated workers (Each app can be deployed with one or more workers)
